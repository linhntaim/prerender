{
    "name": "render-man",
    "version": "1.0.2",
    "description": "A middle man, also a module and an application, wants to render your SPA on the server.",
    "main": "dist/index.js",
    "scripts": {
        "start": "node ./dist/server.js",
        "build": "babel src --out-dir ./dist --delete-dir-on-start --minified --source-maps",
        "test": "nyc mocha --require @babel/register",
        "coverage": "nyc report --reporter=text-lcov | coveralls"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/linhntaim/prerender.git"
    },
    "keywords": [
        "render",
        "server",
        "manager",
        "application",
        "prerender",
        "prerender-io",
        "prerender.io",
        "node",
        "nodejs",
        "node-js",
        "node.js",
        "SPA",
        "SEO"
    ],
    "author": "Nguyen Tuan Linh <contact@linhntaim.com> (https://linhntaim.com)",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/linhntaim/prerender/issues"
    },
    "homepage": "https://github.com/linhntaim/prerender#readme",
    "dependencies": {
        "dotenv-packed": "^1.2.2",
        "illuminate-support": "^0.0.3",
        "prerender": "^5.8.2"
    },
    "devDependencies": {
        "@babel/cli": "^7.13.14",
        "@babel/core": "^7.13.14",
        "@babel/preset-env": "^7.13.12",
        "@babel/register": "^7.13.14",
        "chai": "^4.3.4",
        "coveralls": "^3.1.0",
        "mocha": "^8.3.2",
        "nyc": "^15.1.0"
    }
}
